{
  "name": "sooft-tech-backend",
  "version": "1.0.0",
  "description": "NestJS backend with hexagonal architecture for SOOFT Technology challenge",
  "author": "SOOFT Technology Challenge",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:negative": "jest --config ./test/jest-e2e.json test/e2e/company-negative-core.e2e-spec.ts",
    "test:all": "npm run test && npm run test:e2e && npm run test:negative",
    "db:seed": "ts-node scripts/seed.ts",
    "db:migrate": "ts-node scripts/migrate.ts",
    "db:refresh-mv": "ts-node scripts/refresh-mv.ts",
    "start:otel": "NODE_OPTIONS='-r ./otel-register.cjs' nest start --watch"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.859.0",
    "@faker-js/faker": "^9.9.0",
    "@nestjs/common": "10.4.20",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "10.4.20",
    "@nestjs/platform-express": "10.4.20",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-otlp-http": "^0.26.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^17.2.1",
    "nestjs-pino": "^3",
    "pg": "^8.16.3",
    "pino": "^8",
    "pino-http": "^8",
    "prom-client": "^15.1.3",
    "reflect-metadata": "0.1.14",
    "rxjs": "7.8.2",
    "typeorm": "^0.3.25",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
